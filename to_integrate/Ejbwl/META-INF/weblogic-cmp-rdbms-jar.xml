<?xml version="1.0"?>
<!DOCTYPE weblogic-rdbms-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>
<weblogic-rdbms-jar>
	<weblogic-rdbms-bean>
		<ejb-name>BookCMP</ejb-name>
		<data-source-name>jdbc/ds1</data-source-name>
		<table-map>
			<table-name>BOOKS</table-name>
			<field-map>
				<cmp-field>title</cmp-field>
				<dbms-column>TITLE</dbms-column>
			</field-map>
			<field-map>
				<cmp-field>author</cmp-field>
				<dbms-column>AUTHOR</dbms-column>
			</field-map>
			<field-map>
				<cmp-field>price</cmp-field>
				<dbms-column>PRICE</dbms-column>
			</field-map>
		</table-map>
	</weblogic-rdbms-bean>
</weblogic-rdbms-jar>
